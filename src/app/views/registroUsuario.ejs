  <div class="register-form">
  <h1>REGISTRO DE USUARIOS</h1>
  <form action="addUsers" method="POST">
    <label for="">Nombre Completo</label>
    <input type="text" name="nombre" required>
    <div class= camp>
      <label for="">Tipo de documento</label>
      <select name ="tipoDocumento"  class="select-css" required>
        <option value =""  selected disabled>Selecciona un tipo de documento</option>
        <option value ="Cédula de Ciudadanía (CC)" >Cédula de Ciudadanía (CC)</option>
        <option value="Cédula de extrangería (CE)"  >Cédula de extrangería (CE)</option>
        <option value ="Tarjeta de Identidad (TI)" >Tarjeta de Identidad (TI)</option>
        <option value ="Pasaporte" >Pasaporte</option>
        <option value ="Registro Civil (RC)" >Registro Civil (RC)</option>
        <option value ="Número Único de Identificación Personal (NUIP)" >Número Único de Identificación Personal (NUIP)</option>
        <option value ="Numero de Identificación Personal (NIP)" >Numero de Identificación Personal (NIP)</option>
      </select>
    </div>
      <label for="">Cédula o identificación</label>
      <input type="text" name="cedula" required>
      <label for="">Correo</label>
      <input type="text" name="correo" required>
        <label for="">Teléfono</label>
        <input type="text" name= "telefono" required>

    <label for="rol">Rol</label>
    <select name ="rol" id="rol" class="select-css"  required>
      <option value="Coordinador CDS" selected>Coordinador CDS</option>
      <option value ="Apoyo Telecentro" selected>Apoyo Telecentro</option>
    </select>

    <label for="pass">Contraseña</label>
    <input type="password" name="pass" required>



    <div style ="display: unline;">
      <input type="submit" class="btn-save" value ="Guardar">
    </div>
  </form>
</div>

<script src="//cdn.jsdelivr.net/npm/sweetalert2@10"></script>

  <% if(typeof alert != "undefined"){%>
  <script>
    Swal.fire({
      title:'<%= title %>',
      icon:'<%= icon %>',
      showConfirmButton:'<%= showConfirmButton %>',
      timer: '<%= timer %>'
    }).then(() =>{
      window.location='/<%= ruta %>'
    })
  </script>
<% } %>


