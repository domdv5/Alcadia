  <h1>REGISTRO DE USUARIOS</h1>

  <form id="usersForm">
    <label for="">Nombre CDS o TELECENTRO</label>
    <select  name="IdCds" id="tipo"  class="select-css" required>
      <option value ="" selected disabled>Selecciona un cds o telecentro</option>
      <% for(let i = 0; i < data.length; i++) { %> 
        <option value ="<%= data[i].IdCds %>" ><%= data[i].concatenar %> </option>
        <%  } %> 
      </select>

      

      <label for="">CÃ©dula</label>
      <input type="text" for="validationServer"  id="validationServer01" name="cedula" pattern="[0-9]{7,10}"  title="la cedula debe tener minimo 7 digitos o maximo 10" onchange="documentValidation()" required>
         <div class="invalid-feedback">
        La cedula ya es un usuario activo, por favor digite otra.
        </div>

      <label for="">Correo</label>
      <input type="text" name="correo" required>


      <label for="">TelÃ©fono</label>
      <input type="text" name= "telefono" required>

    <label for="rol">Rol</label>
    <select name ="rol" id="rol" class="select-css"  required>
      <option value="0" selected>General</option>
      <option value ="1" selected>Administrador</option>
    </select>

    <label for="pass">ContraseÃ±a</label>
    <input type="password" name="pass" required>



    <div style ="display: unline;">
      <input type="submit" id="btnLogin" class="btn-save" value ="Guardar">
    </div>
  </form>
