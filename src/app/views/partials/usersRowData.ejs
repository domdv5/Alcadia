  <div class="modal fade" id="exampleModal<%=data.id_usuarios%>" tabindex="-1"
    aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">MODIFICA EL CAMPO REQUERIDO</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
          <form class="usersForm">

            <!-- pendiente -->
            <div class="mb-3">
              <label for="message-text" class="col-form-label"> CDS o Telecentro</label>
              <select name="IdCds" id="tipo" >
                
                <option value="<%= data.id_usuarios%>"><%= data.concatenar%></option>
              </select>
            </div>
            <!-- pendiente -->

            <div class="mb-3">
              <label for="recipient-name" class="col-form-label">Cedula</label>
              <input type="text" name="cedula" value="<%= data.cedula%>">
            </div>

            <div class="mb-3">
              <label for="recipient-name" class="col-form-label">Correo</label>
              <input type="text" name="correo" value="<%= data.correo%>">
            </div>
            <div class="mb-3">
              <label for="recipient-name" class="col-form-label">t√©lefono</label>
              <input type="number" name="telefono" value="<%= data.telefono%>">
            </div>
            <div class="mb-3">
              <label for="recipient-name" class="col-form-label">Rol</label>
              <select name="rol" id="rol" class="select-css" value="<%= data.rol%>">>
                <option value="1" selected>Administrador</option>
                <option value="0" selected>General</option>
              </select>
            </div>

            <div class="modal-footer">
              <button type="button" class="btn btn-secondary" data-bs-dismiss="modal"> Cancelar</button>
              <input type="submit" class="btn btn-primary" value="Aceptar">
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>